{
  "name": "@litmers/cursorflow-orchestrator",
  "version": "0.1.41",
  "description": "Git worktree-based parallel AI agent orchestration system for Cursor",
  "main": "dist/cli/index.js",
  "bin": {
    "cursorflow": "dist/cli/index.js",
    "cursorflow-setup": "dist/cli/setup-commands.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --selectProjects unit",
    "test:unit": "jest --selectProjects unit",
    "test:contract": "jest --selectProjects contract",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:e2e:real": "jest tests/e2e/real-e2e.test.ts --runInBand --testTimeout=300000",
    "test:smoke": "jest tests/smoke --runInBand --testTimeout=120000",
    "test:all": "jest",
    "test:verify": "./scripts/verify-change.sh",
    "test:verify:quick": "./scripts/verify-change.sh --quick",
    "test:verify:full": "./scripts/verify-change.sh --full",
    "test:pipeline": "pnpm test:unit && pnpm test:contract && pnpm test:integration && pnpm test:smoke",
    "prepublishOnly": "pnpm run build && pnpm run validate",
    "postinstall": "node scripts/postinstall.js",
    "validate": "pnpm pack",
    "release": "scripts/release.sh",
    "release:patch": "scripts/release.sh patch",
    "release:minor": "scripts/release.sh minor",
    "release:major": "scripts/release.sh major",
    "prerelease": "npm run build && npm run test && npm run test:smoke && npm run security:check",
    "security:check": "scripts/local-security-gate.sh",
    "security:audit": "pnpm audit",
    "security:audit:fix": "pnpm audit --fix",
    "cf": "node dist/cli/index.js",
    "cf:doctor": "node dist/cli/index.js doctor",
    "cf:run": "node dist/cli/index.js run",
    "cf:monitor": "node dist/cli/index.js monitor",
    "cf:clean": "node dist/cli/index.js clean",
    "cf:resume": "node dist/cli/index.js resume",
    "cf:logs": "node dist/cli/index.js logs",
    "prepare": "husky"
  },
  "keywords": [
    "cursor",
    "cursor-ide",
    "ai-agent",
    "orchestration",
    "git-worktree",
    "parallel-execution",
    "automation",
    "devops",
    "ci-cd"
  ],
  "author": "Eugene Jin <eungjin.cigro@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eungjin-cigro/cursorflow.git"
  },
  "bugs": {
    "url": "https://github.com/eungjin-cigro/cursorflow/issues"
  },
  "homepage": "https://github.com/eungjin-cigro/cursorflow#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "git-http-mock-server": "^2.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/",
    "src/",
    "scripts/",
    "commands/",
    "templates/",
    "examples/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ]
}
